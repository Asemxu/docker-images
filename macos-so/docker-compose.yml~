services:
  macos:
    image: dockurr/macos
    container_name: macos
    environment:
      VERSION: "11"
      RAM_SIZE: "4G"
      CPU_CORES: "4"
    devices:
      - /dev/kvm
      - /dev/net/tun
    cap_add:
      - NET_ADMIN
    ports:
      - 8006:8006
      - 5900:5900/tcp
        - 5900:5900/udp
